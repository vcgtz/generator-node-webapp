{
  "name": "generator-node-webapp",
  "version": "1.0.0",
  "description": "Generator for a webapp using NodeJS, ExpressJS, TailwindCSS, Handlebars, and Mongoose",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write 'generators/app/*.js'",
    "format:check": "prettier --check 'generators/app/*.js'",
    "lint": "eslint 'generators/app/*.js'",
